{
  "functions": {
    "api/members.js": {
      "maxDuration": 10
    },
    "api/admin-auth.js": {
      "maxDuration": 10
    },
    "api/admin-upload.js": {
      "maxDuration": 30
    },
    "api/chat.js": {
      "maxDuration": 15
    }
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}