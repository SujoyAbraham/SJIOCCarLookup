{
  "functions": {
    "api/members.js": {
      "maxDuration": 10
    },
    "api/admin-auth.js": {
      "maxDuration": 10
    },
    "api/admin-upload.js": {
      "maxDuration": 30
    },
    "api/chat.js": {
      "maxDuration": 15
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}